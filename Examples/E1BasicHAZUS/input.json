{
  "Name": "E1 - HAZUS",
  "Author": "Adam Zsarnoczay & Major Zeng",
  "units": {
      "force": "kips",
      "length": "ft"
   },
  "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": false
  },
  "Applications": {
    "Hazard": {
        "Application": "UserInputGM",
        "ApplicationData": {
	    "eventFile": "/Project_general_IM/EventGrid.csv",
	    "motionDir": "/Project_general_IM"
	}
    },	
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "filter": "1-2",
        "buildingSourceFile":"San_Mateo_buildings.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "filenameEVENTgrid": "Project_general_IM/EventGrid.csv",
        "samples": 100,
        "neighbors": 4
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {}
    }],
    "Simulation": {
       "Application": "IMasEDP",
       "ApplicationData": {}
    },
    "DL": {
       "Application": "pelicun",
       "ApplicationData": {
          "DL_Method": "HAZUS MH EQ IM",
          "Realizations": "100",
          "coupled_EDP": true,
          "event_time": "off",
          "detailed_results": false,
          "log_file": true
       }
    }
  }
}
