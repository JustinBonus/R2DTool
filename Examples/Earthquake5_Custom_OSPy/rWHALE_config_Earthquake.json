{
  "Name": "rWHALE_",
  "Author": "Adam Zsarn√≥czay",
  "units": {
      "force": "kips",
      "length": "in",
      "time": "sec"
   },
   "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": true
  },
  "Applications": {
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "filter": "13-14",
        "buildingSourceFile":"input_data/input_params.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "filenameEVENTgrid": "input_data/records/EventGrid.csv",
        "samples": 5,
        "neighbors": 1
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {}
    }],
    "Modeling": {
      "Application": "OpenSeesPyInput",
      "ApplicationData": {
        "mainScript": "cantilever.py",
        "modelPath": "input_data/model"
      }
    },
    "EDP": {
      "Application": "UserDefinedEDP_R",
      "ApplicationData": {
        "EDPspecs": "input_data/EDP_specs.json"
      }
    },
    "Simulation": {
       "Application": "OpenSeesPy-Simulation",
       "ApplicationData": {}
    },
    "UQ": {
       "Application": "Dakota-UQ",
       "ApplicationData": {
          "method": "LHS",
          "samples": 5,
          "type": "UQ",
          "concurrency": 1,
          "keepSamples": true
       }
    },
    "DL": {
            "Application": "pelicun",
            "ApplicationData": {
                "DL_Method": "HAZUS MH EQ",
                "Realizations": 5,
                "detailed_results": false,
                "log_file": true,
                "coupled_EDP": true,
                "event_time": "off",
                "ground_failure": false
            }
        }
    }
}
