{
  "Name": "AnchorageM7.0",
  "Author": "Adam Zsarn√≥czay",
  "units": {
      "force": "kips",
      "length": "ft",
      "acceleration": "g"
   },
  "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": false
  },
  "Applications": {
    "Hazard": {
        "Application": "UserInputGM",
        "ApplicationData": {
	    "eventFile": "input_data/records/EventGrid.csv",
	    "motionDir": "input_data/records"
	}
    },		
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "filter": "1-10",
        "buildingSourceFile":"input_data/AnchorageBuildings.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "filenameEVENTgrid": "input_data/records/EventGrid.csv",
        "samples": 3,
        "neighbors": 4
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {}
    }],
    "Modeling": {
      "Application": "MDOF-LU",
      "ApplicationData": {
         "hazusData":"input_data/HazusData.txt",
         "stdStiffness": 0.1,
         "stdDamping": 0.1
      }
    },
    "EDP": {
      "Application": "StandardEarthquakeEDP_R",
      "ApplicationData": {}
    },
    "Simulation": {
       "Application": "OpenSees-Simulation_R",
       "ApplicationData": {}
    },
    "UQ": {
       "Application": "Dakota-UQ",
       "ApplicationData": {
          "method": "LHS",
          "samples": 3,
          "type": "UQ",
          "concurrency": 1,
          "keepSamples": true
       }
    },
    "DL": {
       "Application": "pelicun",
       "ApplicationData": {
          "DL_Method": "HAZUS MH EQ",
          "Realizations": "1000",
          "coupled_EDP": false,
          "event_time": "off",
          "detailed_results": false,
          "log_file": true
       }
    }
  }
}


