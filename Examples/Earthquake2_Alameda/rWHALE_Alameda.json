{
  "Name": "Earthquake - Alameda",
  "Author": "Tamika Bassman",
  "units": {
      "force": "kips",
      "length": "in",
      "acceleration": "g"
   },
  "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": true
  },
  "Applications": {
    "Hazard": {
        "Application": "UserInputGM",
        "ApplicationData": {
	    "eventFile": "input_data/IMs/EventGrid.csv",
	    "motionDir": "input_data/IMs"
	}
    },	
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "filter": "1-20",
        "buildingSourceFile":"input_data/all_bldgs.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "filenameEVENTgrid": "input_data/IMs/EventGrid.csv",
        "samples": 10000,
        "neighbors": 4,
        "filter_label": "SoilType"
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {}
    }],
    "Simulation": {
       "Application": "IMasEDP",
       "ApplicationData": {}
    },
    "DL": {
       "Application": "pelicun",
       "ApplicationData": {
          "DL_Method": "HAZUS MH EQ IM",
          "Realizations": "10000",
          "coupled_EDP": true,
          "event_time": "off",
          "detailed_results": false,
          "log_file": true,
          "ground_failure": true
       }
    }
  }
}


